{
  "name": "codeinwp/themeisle-companion",
  "description": "Orbit Fox Companion",
  "keywords": [
    "wordpress"
  ],
  "homepage": "https://themeisle.com/plugins/themeisle-companion/",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "ThemeIsle Team",
      "email": "friends@themeisle.com.com",
      "homepage": "https://themeisle.com"
    }
  ],
  "type": "wordpress-plugin",
  "support": {
    "issues": "https://github.com/Codeinwp/themeisle-companion/issues",
    "source": "https://github.com/Codeinwp/themeisle-companion"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3",
    "wp-coding-standards/wpcs": "^2.3",
    "phpcompatibility/php-compatibility": "^9",
    "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
    "automattic/vipwpcs": "^2.3"
  },
  "scripts": {
    "lint": "phpcs -n",
    "format": "phpcbf"
  },
  "require": {
    "codeinwp/full-width-page-templates": "master",
    "codeinwp/themeisle-sdk": "^3.3",
    "enshrined/svg-sanitize": "^0.19.0",
    "mailerlite/mailerlite-api-v2-php-sdk": "^0.2.3"
  },
  "autoload": {
    "files": [
      "vendor/codeinwp/themeisle-sdk/load.php",
      "obfx_modules/content-forms/load.php",
      "obfx_modules/elementor-extra-widgets/load.php"
    ]
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "5.6"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "installer-disable": "true"
  },
  "minimum-stability": "dev"
}
